        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client"
        mc:Ignorable="d"
        Title="Dialogue" Height="183.099" Width="300">
    <Grid>
        <TextBox x:Name="txtResult" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="272" Margin="10,78,0,0"/>
        <Label x:Name="lblAsk" Content="Label" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,17,0,0" FontSize="14"/>
        <Button x:Name="btnSubmit" Content="OK" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="10,122,0,0" Click="btnSubmit_Click"/>

    </Grid>
</Window>
